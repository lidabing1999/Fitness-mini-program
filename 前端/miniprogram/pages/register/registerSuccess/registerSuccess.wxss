/* miniprogram/pages/register/registerSuccess/registerSuccess.wxss */
page {
  width: 100%;
  height: 100%;
  color: #6f6f70;
  background-color: #e9edf1;
  overflow: hidden;
}

.successIcon {
  text-align: center;
  margin-top: 80rpx;
}

.congratulation {
  width: 100%;
  text-align: center;
  font-size: 45rpx;
  letter-spacing: 2px;
  font-weight: bold;
  color: #646464;
}

.backContainer {
  width: 100%;
  text-align: center;
  font-size: 32rpx;
  letter-spacing: 2px;
  margin-top: 20rpx;
  color: #9a9a9a;
}

.time {
  color: #3d6ce0;
}

.backToPersonalCenter {
  width: 60%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 38rpx;
  margin: 60rpx auto 0 auto;
  color: #fff;
  background-color: #3d6ce0;
}
